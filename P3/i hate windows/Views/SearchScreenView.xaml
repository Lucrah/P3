            </Border.Effect>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="28*"/>
                    <RowDefinition Height="20"/>
                    <RowDefinition Height="221*"/>
                    <RowDefinition Height="20*"/>
                    <RowDefinition Height="601*"/>
                </Grid.RowDefinitions>
            </Grid>
        </Border>

    </Grid>
</UserControl>
