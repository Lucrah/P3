<Window x:Class="P3.Views.SearchScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:P3.Viewmodels"
        DataContext="{DynamicResource ViewModelSearchScreen}"
        Title="SearchScreen" Height="1080" Width="1920" ResizeMode="CanMinimize">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="486,651,0,0" TextWrapping="Wrap" Text="SearchScreen" VerticalAlignment="Top" Width="120"/>

    </Grid>
    
    <Window.Resources>
        <vm:ViewModelSearchScreen x:Key="ViewModelSearchScreen"/>
    </Window.Resources>
</Window>
