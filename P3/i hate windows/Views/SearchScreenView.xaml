        <Border BorderThickness="2" Margin="0,5.833,0,70.924" Background="#FF2C2C2C" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
            <Border.Effect>
                <DropShadowEffect/>
            </Border.Effect>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8*"/>
                    <ColumnDefinition Width="13*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="27*"/>
                    <RowDefinition Height="20"/>
                    <RowDefinition Height="24*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="30*"/>
                    <RowDefinition Height="30*"/>
                    <RowDefinition Height="30*"/>
                    <RowDefinition Height="126*"/>
                    <RowDefinition Height="20*"/>
                    <RowDefinition Height="100*"/>
                    <RowDefinition Height="487*"/>
                </Grid.RowDefinitions>
                <Rectangle Fill="#FF005430" Margin="0,0.07,0,-7.777" Stroke="Black" Grid.Row="2" d:LayoutOverrides="LeftMargin, RightMargin" StrokeThickness="0" Grid.RowSpan="5" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,30.965,0,-0.343" Stroke="Black" Grid.Row="7" StrokeThickness="0" d:LayoutOverrides="LeftMargin, RightMargin" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,-0.316,0,-0.102" Stroke="Black" Grid.Row="9" StrokeThickness="0" d:LayoutOverrides="LeftMargin, RightMargin" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,21.644,0,0" Stroke="Black" Grid.Row="10" StrokeThickness="0" d:LayoutOverrides="LeftMargin, RightMargin" Height="100" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,139.432,0,0" Stroke="Black" Grid.Row="10" StrokeThickness="0" Height="80.31" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,239.245,0,147.328" Stroke="Black" Grid.Row="10" StrokeThickness="0" Grid.ColumnSpan="2"/>
                <Rectangle Fill="#FF005430" Margin="0,0,0,25.786" Stroke="Black" Grid.Row="10" StrokeThickness="0" Height="100" VerticalAlignment="Bottom" Grid.ColumnSpan="2"/>
                <CheckBox x:Name="checkBox" Content=" Villa" Margin="13,0,35,5.546" Grid.Row="4" FontSize="16" Foreground="White"/>
                <TextBlock x:Name="textBlock" Grid.Row="2" TextWrapping="Wrap" Foreground="White" FontSize="18.667" Grid.Column="1" HorizontalAlignment="Left" Width="70"><Run Text="Hustype"/><LineBreak/><Run/></TextBlock>
                <CheckBox x:Name="checkBox_Copy" Content=" Liebhaverejendom" Margin="13,0,30,5.546" Grid.Row="5" FontSize="16" Foreground="White"/>
                <CheckBox x:Name="checkBox_Copy1" Content=" Fritidsejendom" Margin="13,0,35,5.546" Grid.Row="6" FontSize="16" Foreground="White"/>
                <CheckBox x:Name="checkBox_Copy2" Content=" Andelsbolig" Margin="0,5.546,0,0" Grid.Row="4" FontSize="16" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Width="160"/>
                <CheckBox x:Name="checkBox_Copy3" Content=" Rækkehus" Margin="0,5.546,0,0" Grid.Row="5" FontSize="16" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Width="165"/>
