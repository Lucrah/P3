<Window x:Class="P3.Views.PropertyInfoScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:P3.Viewmodels"
        DataContext="{DynamicResource ViewModelPropertyScreen}"
        Title="PropertyInfoScreen" Height="1080" Width="1920" ResizeMode="CanMinimize">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="407,591,0,0" TextWrapping="Wrap" Text="PropertyInfoScreen" VerticalAlignment="Top" Width="120"/>

    </Grid>
    <Window.Resources>
        <vm:ViewModelPropertyScreen x:Key="ViewModelPropertyScreen"/>
    </Window.Resources>
</Window>
