                <TextBlock x:Name="textBlock_Copy" Grid.Row="2" TextWrapping="Wrap" Foreground="White" FontSize="18.667" Grid.Column="1" HorizontalAlignment="Left" Width="70"><Run Text="Hustype"/><LineBreak/><Run/></TextBlock>
                <TextBlock x:Name="textBlock_Copy1" Grid.Row="7" TextWrapping="Wrap" Foreground="White" FontSize="18.667" Grid.Column="1" HorizontalAlignment="Left" Width="70" Height="23.952" Margin="0,30.965,0,0" VerticalAlignment="Top" Text="Pris"/>
                <controls:RangeSlider Margin="10,0,138,38.287" Grid.Row="7" VerticalAlignment="Bottom" Grid.ColumnSpan="2" TickPlacement="BottomRight" IsMoveToPointEnabled="True">
                </controls:RangeSlider>
            </Grid>
        </Border>
        <Border BorderThickness="2" Margin="10,5.833,0,70.924" IsEnabled="False" Background="#FF2C2C2C" Grid.Column="1" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
